pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'checking out from git'
            }
        }

        stage('compile') {
            steps {
                echo 'compiling'
            }
        }

        stage('test') {
            steps {
                echo 'testing'
            }
        }

        stage('package') {
            steps {
                echo 'package'
            }
        }
    }
}